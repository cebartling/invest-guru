type Query {
  userByEmail(email: String!): User
}

type Mutation {
  createUser(userInput: UserInput!): User
}

type User {
  id: ID!
  givenName: String!
  familyName: String!
  locale: String!
  email: String!
}

input UserInput {
  givenName: String!
  familyName: String!
  locale: String!
  email: String!
}